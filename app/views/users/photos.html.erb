<div class="mdl-grid">
	<div id="prefs-div-title" class="mdl-cell mdl-cell-12-col">
		<h2 class="mdl-card__title-text">Photos</h2>
	</div>
	<% @array_img_links.each do |img| %>
		<div class="mdl-cell mdl-cell--6-col mdl-cell-6-col-tablet">
			<div class="mdl-card mdl-shadow--2dp div-photos-singles">
  			<div class="mdl-card__title mdl-card--expand"></div>
				<img class="photos-singles" src="<%= img %>">	
			</div>
		</div>
	<% end %>
	<div id="prefs-div-cta-button" class="mdl-cell mdl-cell-1-col">
		<a href="/users/<%= @current_user.referrer_id %>/share"><button class="mdl-button mdl-js-button mdl-button--raised mdl-button--accent mdl-js-ripple-effect">Share</button></a>
	</div>
</div>